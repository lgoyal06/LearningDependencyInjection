<?xml version="1.0" encoding="UTF-8"?>
<beans>
	<bean id="bImpl">
		<name>com.lalit.api.di.design.setterInjection.example.BImpl</name>
		<ConstructorInjection>
			<arg value-ref="aImplVersion1" type="com.lalit.api.di.design.setterInjection.example.AInterface" />
			<arg value-ref="cImplVersion1" type="com.lalit.api.di.design.setterInjection.example.ICImpl"/>
			<arg value="10" type="int"/>
			<arg value="false" type="boolean"/>
		</ConstructorInjection>
		<SetterInjection>
			<property id="aClass1" value-ref="aImplVersion2" />
		</SetterInjection>
	</bean>
	<bean id="aImplVersion1">
		<name>com.lalit.api.di.design.setterInjection.example.AImplVersion1</name>
		<SetterInjection>
			<property id="cClass" value-ref="cImplVersion1" />
		</SetterInjection>
	</bean>
	<bean id="aImplVersion2">
		<name>com.lalit.api.di.design.setterInjection.example.AImplVersion2</name>
		<ConstructorInjection>
			<arg  value-ref="cImplVersion1"  type="com.lalit.api.di.design.setterInjection.example.ICImpl"/>
		</ConstructorInjection>
		<SetterInjection>
			<property id="x" dataType="int" value="10" />
		</SetterInjection>
	</bean>
	<bean id="cImplVersion1">
		<name>com.lalit.api.di.design.setterInjection.example.CImplVersion1</name>
	</bean>
</beans>
